import java.util.Scanner;

class tmp {
    public static void main(String[] args) 
    {
		try
		{
		    PrintWriter saveFile = new PrintWriter(new FileWriter("dictionary.txt"));
		    Scanner sc = new Scanner(new BufferedReader(new FileReader("dictionaryraw.txt")));
		    sc.useDelimiter("\n");
	  
		    String word;
		    String temp;
		    while(sc.hasNextLine())
			{
			    word = sc.nextLine().trim().toLowerCase();
			    
			    if(word.indexOf(" ") == -1)
				saveFile.println(word);
			    while(sc.hasNext("\\s*.*\\d+\\s*"))
				{
				    temp = sc.nextLine().replaceAll("\\d+\\S*", "").trim();
				    if(word.indexOf(" ") == -1)
					saveFile.println(temp);
				}
			    
			    if(word.indexOf(" ") == -1)
				saveFile.println("");
			}
	   
		}
		catch(Exception E) {System.out.println("Error");}
    }
}